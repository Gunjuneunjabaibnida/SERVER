one_set("UTC");return "UTC";}}
function Tmr($tmr){
 $col = [b,c,d,k,m,u];
 $sym = [' ->>> ',' <<<- ',' ->>> ',' <<<- '];
 $text = ['You-claim-again '];
 $text1 = [' minutes '];
 $text2 = [' seconds '];
 $timr = time()+$tmr;
 $a = 0;
 while(true){
  $a +=1;
  $x = $col[array_rand($col)];
  $timerclaim = date('i:s',$res);
 
  $res=$timr-time();
  if($res < 1) {
   break;
  }
  print $x.$sym[$a % 4].$x.$text[$a % 1].$x.$timerclaim.$x.$sym[$a % 4]." \r";
  usleep(100000);
 }
 print "\r                                   \r";
}
